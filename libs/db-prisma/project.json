{
  "name": "db-prisma",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/db-prisma/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "dependsOn": ["generate"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/db-prisma",
        "tsConfig": "libs/db-prisma/tsconfig.lib.json",
        "packageJson": "libs/db-prisma/package.json",
        "main": "libs/db-prisma/src/index.ts",
        "assets": ["libs/db-prisma/*.md"]
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "pnpm run generate"
          }
        ],
        "cwd": "libs/db-prisma"
      }
    },
    "migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run migrate",
        "cwd": "libs/db-prisma"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma studio",
        "cwd": "libs/db-prisma"
      }
    }
  }
}
